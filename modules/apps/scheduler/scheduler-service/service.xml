<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.3.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_3_0.dtd">

<service-builder auto-namespace-tables="false" mvcc-enabled="true" package-path="com.liferay.scheduler">
	<author>Alessio Antonio Rendina</author>
	<namespace>Scheduler</namespace>
	<entity local-service="true" name="SchedulerProcess" remote-service="true">

		<!-- PK fields -->

		<column name="schedulerProcessId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="name" type="String" />
		<column name="type" type="String" />
		<column name="typeSettings" type="String" />
		<column name="system" type="boolean" />
		<column name="active" type="boolean" />
		<column name="cronExpression" type="String" />
		<column name="startDate" type="Date" />
		<column name="endDate" type="Date" />

		<!-- Order -->

		<order by="desc">
			<order-column name="modifiedDate" />
		</order>

		<!-- Finder methods -->

		<finder name="CompanyId" return-type="Collection">
			<finder-column name="companyId" />
		</finder>
		<finder name="C_N" return-type="SchedulerProcess" unique="true">
			<finder-column name="companyId" />
			<finder-column name="name" />
		</finder>
		<finder name="C_T" return-type="Collection">
			<finder-column name="companyId" />
			<finder-column name="type" />
		</finder>
	</entity>
	<entity local-service="true" name="SchedulerProcessLog" remote-service="true">

		<!-- PK fields -->

		<column name="schedulerProcessLogId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="schedulerProcessId" type="long" />
		<column name="error" type="String" />
		<column name="output" type="String" />
		<column name="startDate" type="Date" />
		<column name="endDate" type="Date" />
		<column name="status" type="int" />

		<!-- Order -->

		<order by="desc">
			<order-column name="modifiedDate" />
		</order>

		<!-- Finder methods -->

		<finder name="SchedulerProcessId" return-type="Collection">
			<finder-column name="schedulerProcessId" />
		</finder>
		<finder name="S_S" return-type="Collection">
			<finder-column name="schedulerProcessId" />
			<finder-column name="status" />
		</finder>
	</entity>
	<exceptions>
		<exception>DuplicateSchedulerProcess</exception>
		<exception>SchedulerProcessEndDate</exception>
		<exception>SchedulerProcessName</exception>
		<exception>SchedulerProcessStartDate</exception>
	</exceptions>
</service-builder>